/**app.wxss**/
.container {
  height: calc(100% - 120rpx);
  display: flex;
  flex-direction: column;
  padding: 10rpx 0;
  box-sizing: border-box;
  padding-bottom:  env(safe-area-inset-bottom);
  margin-bottom: 120rpx;
} 
::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
    display: none;
}
